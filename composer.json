{
	"name"       : "justintadlock/hybrid-core",
	"description": "Hybrid Core WordPress theme framework",
	"keywords"   : ["wordpress"],
	"homepage"   : "https://themehybrid.com/hybrid-core",
	"license"    : "GPL-2.0+",
	"authors"    : [
		{
			"name"    : "Justin Tadlock",
			"email"   : "justin@justintadlock.com",
			"homepage": "http://justintadlock.com"
		}
	],
	"support"    : {
		"issues": "https://github.com/justintadlock/hybrid-core/issues",
		"forum" : "https://themehybrid.com/board/forums/hybrid-core"
	},
	"autoload"   : {
		"psr-4": {
			"Hybrid\\": "src/"
		},
		"files": [
			"hybrid.php",
			"src/functions-attr.php",
			"src/functions-context.php",
			"src/functions-deprecated.php",
			"src/functions-filters.php",
			"src/functions-fonts.php",
			"src/functions-formats.php",
			"src/functions-head.php",
			"src/functions-i18n.php",
			"src/functions-layouts.php",
			"src/functions-meta.php",
			"src/functions-scripts.php",
			"src/functions-setup.php",
			"src/functions-sidebars.php",
			"src/functions-styles.php",
			"src/functions-templates.php",
			"src/functions-utility.php",
			"src/template-comments.php",
			"src/template-general.php",
			"src/template-media.php",
			"src/template-post.php",
			"src/template.php",
			"src/admin/functions-admin.php"
		]
	},
	"require": {
		"psr/container": "^1.0"
	}
}
